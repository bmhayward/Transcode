# copy vtExtras.zip to ~/Library/Application Scripts/com.videotranscode.transcode
# copy all Extras to /Transcode/Extras and rename to .command

function echo_Msg () {
	# ${1}: message to echo
	# ${2}: flag to suppress echo
	
	if [ $# -eq 1 ]; then
		echo "${1}"											# echo to the Terminal
