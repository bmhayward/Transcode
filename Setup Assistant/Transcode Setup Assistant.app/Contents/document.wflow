# build out the directory structure

function echo_Msg () {
	# ${1}: message to echo
	# ${2}: flag to suppress echo
	
	if [ $# -eq 1 ]; then
		echo "${1}"											# echo to the Terminal
	fi
    echo "${1}" 2&gt;&amp;1 | logger -t transcode.install			# echo to syslog
}

function update_Prefs () {
	if [ -e "${prefPath}" ]; then
		echo_Msg "Building preferences..." ""
			
		declare -a prefs

		let i=0
		while IFS='' read -r lineData || [[ -n "${lineData}" ]]; do					# read in the preferences from the prefs file
		    prefs[i]="${lineData}"
		    ((++i))
		done &lt; "${prefPath}"

		readonly outExt="${prefs[0]}"												# get the transcode file extension
		readonly deleteWhenDone="${prefs[1]}"										# what to do with the original files when done
		readonly movieTag="${prefs[2]}"												# Finder tags for movie files
		readonly tvTag="${prefs[3]}"												# Finder tags for TV show files
		readonly convertedTag="${prefs[4]}"											# Finder tags for original files that have been transcoded		
		readonly renameFile="${prefs[5]}"											# whether or not to auto-rename files
		readonly movieFormat="${prefs[6]}"											# movie rename format
		readonly tvShowFormat="${prefs[7]}"											# TV show rename format
		readonly plexPath="${prefs[8]}"												# where to put the transcoded files in Plex
		readonly sshUser="${prefs[9]}"												# get the ssh username
		readonly rsyncPath="${prefs[10]}"											# get the path to the rsync Remote directory
		readonly ingestPath="${prefs[11]}"											# get the path to the ingest directory
		readonly extrasTag="${prefs[12]}"											# Finder tags for Extra show files
		readonly outQuality=""														# Output quality setting to use
		
		rm -f "${prefPath}"  # delete the Pref.txt file
	else
		echo_Msg "Updating preferences..." ""
		
		readonly outExt="m4v"														# get the transcode file extension
		readonly deleteWhenDone="false"												# what to do with the original files when done
		readonly movieTag="purple,Movie,VT"											# Finder tags for movie files
		readonly tvTag="orange,TV Show,VT"											# Finder tags for TV show files
		readonly convertedTag="blue,Converted"										# Finder tags for original files that have been transcoded		
		readonly renameFile="auto"													# whether or not to auto-rename files
		readonly movieFormat=""														# movie rename format
		readonly tvShowFormat="{n} - {'"'"'s'"'"'+s.pad(2)}e{e.pad(2)} - {t}"		# TV show rename format
		readonly plexPath=""														# where to put the transcoded files in Plex
		readonly sshUser=""															# get the ssh username
		readonly rsyncPath=""														# get the path to the rsync Remote directory
		readonly ingestPath=""														# get the path to the ingest directory
		readonly extrasTag="yellow,Extra,VT"										# Finder tags for Extra show files
		readonly outQuality=""														# Output quality setting to use
	fi
	
	printf "${outExt}\n${deleteWhenDone}\n${movieTag}\n${tvTag}\n${convertedTag}\n${renameFile}\n${movieFormat}\n${tvShowFormat}\n${plexPath}\n${sshUser}\n${rsyncPath}\n${ingestPath}\n${extrasTag}\n${outQuality}" &gt;&gt; "${prefPath}" 
}

libPath="${HOME}/Library"

if [ ! -d "$DIRECTORY" ]; then
	echo_Msg "Building Transcode directory..." ""

	mkdir -p "${1}Transcode"
	mkdir -p "${1}Transcode/Convert"
	mkdir -p "${1}Transcode/Subtitles"
	mkdir -p "${1}Transcode/Logs"
	mkdir -p "${1}Transcode/Crops"
	mkdir -p "${1}Transcode/Completed"
	mkdir -p "${1}Transcode/Completed/Originals"
	mkdir -p "${1}Transcode/Extras"
	mkdir -p "${1}Transcode/Remote"
fi

prefPath="${1}Transcode/Prefs.txt"	# create the default preferences file
update_Prefs

# make sure ~/Library/LaunchAgents and ~/Library/Application Support/Transcode directories exist

if [ ! -d "${libPath}/LaunchAgents" ]; then
	echo_Msg "Creating /LaunchAgents" ""
	
	mkdir -p "${libPath}/LaunchAgents"
fi

if [ ! -d "${libPath}/Application Support/Transcode" ]; then
	echo_Msg "Creating /Application Support/Transcode" ""
	
	mkdir -p "${libPath}/Application Support/Transcode"
fi

if [ ! -d "${libPath}/Application Scripts/com.videotranscode.transcode" ]; then
	echo_Msg "Creating /Application Scripts/com.videotranscode.transcode" ""
	
	mkdir -p "${libPath}/Application Scripts/com.videotranscode.transcode"
fi</string>
